  "homepage": "https://market.ionic.io/themes/ionproperty",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build --prod && html-minifier www/index.html --html5 --remove-comments --collapse-whitespace --conservative-collapse --remove-attribute-quotes --collapse-inline-tag-whitespace --minify-js=true --trim-custom-fragments --remove-tag-whitespace -o www/index.html && npm run copy-redirects",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "copy-redirects": "cp _headers _redirects www/"
