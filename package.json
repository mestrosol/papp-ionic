    "@angular/forms": "5.0.3",
    "@angular/http": "5.0.3",
    "@angular/platform-browser": "5.0.3",
    "@angular/platform-browser-dynamic": "5.0.3",
    "@ionic/storage": "2.1.3",
    "cordova-sqlite-storage": "^2.1.4",
    "ionic-angular": "3.9.2",
    "ionicons": "^3.0.0",
    "leaflet": "^1.0.3",
    "rxjs": "5.5.2",
    "sw-toolbox": "^3.6.0",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.0",
    "ionic": "3.19.0",
